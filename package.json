{
  "name": "squid-evm-template",
  "private": true,
  "scripts": {
    "build": "rm -rf lib && tsc",
    "update": "npx npm-check-updates --filter /subsquid/ --upgrade && npm i -f",
    "db:migrate": "npx squid-typeorm-migration apply",
    "squid-etl:start": "node lib/processor.js",
    "squid-api:start": "squid-graphql-server"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.6.1",
    "@subsquid/archive-registry": "^2.1.4",
    "@subsquid/big-decimal": "^0.0.0",
    "@subsquid/evm-processor": "^0.3.2",
    "@subsquid/file-store-csv": "^0.2.0",
    "dotenv": "^16.0.0",
    "ethers": "^5.6.4"
  },
  "devDependencies": {
    "@subsquid/evm-typegen": "2.0.4",
    "@typechain/ethers-v5": "^10.1.0",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "typechain": "^8.1.0",
    "typescript": "~4.6.2"
  }
}
